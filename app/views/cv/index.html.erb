<h1>curriculum vitae</h1>
<h2>Educations</h2>
    <table class="table table-hover">
      <thead>
      <tr>
        <th>Education</th>
        <th>School</th>
        <th>Started</th>
        <th>Finished</th>
      </tr>
      </thead>
      <tbody>
        <% @edus.each do |edu|  %>
        <tr>
          <td><%= edu.name %></td>
          <td><%= edu.school %></td>
          <td><%= edu.start_date %></td>
          <td><%= edu.end_date %></td>
        </tr>
        <% end %>
      </tbody>
    </table>


<h2>jobs</h2>
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Company</th>
      <th>Position</th>
      <th>Start</th>
      <th>End</th>
    </tr>
    </thead>
    <tbody>
    <% @jobs.each do |job|  %>
        <tr>
          <td><%= job.company %></td>
          <td><%= job.position %></td>
          <td><%= job.start_date %></td>
          <td><%= job.end_date %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
